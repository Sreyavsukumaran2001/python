   "source": [
    "#gcd of two numbers\n",
    "x=float(input(\"Enter the firt number:\"))\n",
    "y=float(input(\"Enter the second number:\"))\n",
    "if(x<y):\n",
    "    c=x\n",
    "else:\n",
    "    c=y\n",
    "while(c>0):\n",
    "    if(x%c==0)and(y%c==0):\n",
    "        print(\"GCD of\",x,\"and\",y,\"is\",c)\n",
    "        break\n",
    "    c=c-1\n"
   ]
