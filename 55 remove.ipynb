   "source": [
    "#55 from a list of integers create a list removing even numbers:\n",
    "evenlist=[10,15,39,42,53,67,82,93]\n",
    "print(\"list items=\",evenlist)\n",
    "for i in evenlist:\n",
    "    if(i%2==0):\n",
    "        evenlist.remove(i)\n",
    "print(\"List items after removing even items=\",evenlist)\n"
   ]
