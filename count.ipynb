   "source": [
    "#39 count the number of characterstics in a string\n",
    "s=input(\"Enter the string:\")\n",
    "freq={}\n",
    "for i in s:\n",
    "    if i in freq:\n",
    "         freq[i]+=1\n",
    "    else:\n",
    "        freq[i]=1\n",
    "print(\"count of all characters in the string:\\n\" \n",
    "      + str(freq)) \n"
   ]
