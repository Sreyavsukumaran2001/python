   "source": [
    "#reverse a integer number\n",
    "a=int(input(\"Enter the number:\"))\n",
    "rev=0    \n",
    "while(a>0):    \n",
    "    d=a%10\n",
    "    rev=(rev*10)+d\n",
    "    a=a//10\n",
    "print(\"reverse of the number is:\",rev)\n",
    "    "
   ]
