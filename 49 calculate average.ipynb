   "source": [
    "# 49 To calculate the average value of the numbers in given tuple\n",
    "t= tuple(input('Enter the numbers : ').split(','))\n",
    "l=len(t)\n",
    "c=0\n",
    "for i in range(l):\n",
    "    c=int(t[i])+c\n",
    "    avg=c/l\n",
    "print(\"the average of the above given numbers are:\",avg)"
   ]
