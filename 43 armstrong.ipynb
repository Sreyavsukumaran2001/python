   "source": [
    "#43 check the given number is armstrong or not\n",
    "n=int(input(\"Enter the number:\"))\n",
    "sum=0\n",
    "temp=n\n",
    "while temp>0:\n",
    "    digit = temp%10\n",
    "    sum +=digit ** 3\n",
    "    temp//=10\n",
    "if n==sum:\n",
    "    print(n,\"is an Armstrong number\")\n",
    "else:\n",
    "    print(n,\"is not an Armstrong number\")"
   ]
